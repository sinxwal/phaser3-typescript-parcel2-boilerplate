{
  "name": "phaser3-typescript-parcel2-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for Phaser 3 using Parceljs v2",
  "scripts": {
    "start": "rimraf dist && parcel serve ./src/index.html -p 8000",
    "build": "rimraf dist && parcel build ./src/index.html --dist-dir dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinxwal/phaser3-typescript-parcel2-boilerplate.git"
  },
  "keywords": [
    "phaser",
    "phaser3",
    "parcel",
    "parcel2",
    "typescript",
    "linter",
    "eslint",
    "prettier",
    "boilerplate",
    "template",
    "starter"
  ],
  "author": "sinxwal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sinxwal/phaser3-typescript-parcel2-boilerplate/issues"
  },
  "homepage": "https://github.com/sinxwal/phaser3-typescript-parcel2-boilerplate#readme",
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.0.0-rc.0",
    "@parcel/transformer-image": "^2.0.0-rc.0",
    "@parcel/transformer-typescript-tsc": "^2.0.0-rc.0",
    "@parcel/transformer-webmanifest": "^2.0.0-rc.0",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "parcel": "^2.0.0-rc.0",
    "parcel-reporter-static-files-copy": "^1.3.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "phaser": "^3.55.2",
    "workbox-core": "^6.2.2",
    "workbox-precaching": "^6.2.2",
    "workbox-routing": "^6.2.2",
    "workbox-strategies": "^6.2.2"
  },
  "staticFiles": {
    "staticPath": "public",
    "staticOutPath": "assets"
  }
}
